(
    top
    {
        type            patch;
        nFaces          1500;
        startFace       1498000;
    }
    bottom
    {
        type            patch;
        nFaces          1500;
        startFace       1499500;
    }
    sideRight
    {
        type            zeroGradient;
        inGroups        1(zeroGradient);
        nFaces          500;
        startFace       1501000;
        faces           ( ( 1 2 6 5 ) );
    }
    sideLeft
    {
        type            symmetryPlane;
